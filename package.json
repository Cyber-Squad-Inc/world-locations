{
  "name": "@cyber_squad_inc/world-locations",
  "version": "1.0.1",
  "description": "Offline world dataset (countries, states, cities) with flags, phone codes, and ISO codes â€” maintained by Cyber Squad Inc.",
  "main": "src/index.ts",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src/",
    "dist/",
    "out/world-locations.full.json.gz"
  ],
  "keywords": [
    "world",
    "locations",
    "countries",
    "states",
    "cities",
    "typescript",
    "nextjs",
    "esm",
    "cyber-squad-inc"
  ],
  "author": "Cyber Squad Inc <contact@cybersquad.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyber-Squad-Inc/world-locations.git"
  },
  "bugs": {
    "url": "https://github.com/Cyber-Squad-Inc/world-locations/issues"
  },
  "homepage": "https://github.com/Cyber-Squad-Inc/world-locations#readme",
  "scripts": {
    "build:full": "node src/build-world-locations.js",
    "merge": "node src/merge.js",
    "validate": "node --loader ts-node/esm src/validate.ts",
    "start": "npm run build:full && npm run merge",
    "build:types": "tsc",
    "prepublishOnly": "npm run start && npm run build:types && rm -rf out/countries"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.5",
    "p-queue": "^7.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
